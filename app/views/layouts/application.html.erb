<!DOCTYPE html>
<html>
	<head>
		<title> Flight Dog: <%= @title %> </title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets'%>
    <%= javascript_include_tag "jquery-1.6.2", "rails", "highcharts" %>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag 'jquery-1.6.2.js', 'jquery-ui-1.8.14.custom.min.js', 'autocomplete-rails.js', 'rails.js'  %>
    <%= stylesheet_link_tag 'jquery-ui-1.8.4.custom.css' %>
	</head>
	<body>
    <div class = "container">
      <section>
        <table>
          <tr>
            <td>
              <%= render 'layouts/logo'%>
            </td>
            <td>
              <%= yield %>
            </td>
          </tr>
        </table>
			</section>
			<%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>