<!DOCTYPE html>
<html>
	<head>
		<title> FarePlots: <%= @title %> </title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets'%>
    <%= javascript_include_tag :defaults %>`
    <%= javascript_include_tag "jquery-1.6.2", "application", "rails", "highcharts", "jquery-ui-1.8.16.custom.min.js", "raphael-min.js", "g.raphael-min.js", "g.line-min.js" %>
    <%= stylesheet_link_tag 'cupertino/jquery-ui-1.8.16.custom.css' %>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-25823313-1']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>
	</head>
	<body>
    <div class = "container">
      <section>
        <table>
          <tr>
            <td>
              <%= render 'layouts/logo'%>
            </td>
            <td>
              <%= yield %>
            </td>
          </tr>
        </table>
			</section>
			<%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>

