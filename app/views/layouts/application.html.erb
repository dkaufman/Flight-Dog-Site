<!DOCTYPE html>
<html>
	<head>
		<title> FarePlots: <%= @title %> </title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets'%>
    <%= javascript_include_tag :defaults %>`
    <%= javascript_include_tag "jquery-1.6.2", "application", "rails", "highcharts", "jquery-ui-1.8.16.custom.min.js", "raphael-min.js", "g.raphael-min.js", "g.line-min.js" %>
    <%= stylesheet_link_tag 'cupertino/jquery-ui-1.8.16.custom.css' %>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <%= render ''layouts/googleanalytics'' if RAILS_ENV == 'production' %>
	</head>
	<body>
    <div class = "container">
      <section>
        <table>
          <tr>
            <td>
              <%= render 'layouts/logo'%>
            </td>
            <td>
              <%= yield %>
            </td>
          </tr>
        </table>
			</section>
			<%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>

